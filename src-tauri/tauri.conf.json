{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "tauri-sample",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "updater": {
      "windows": {
        "installMode": "basicUi"
      },
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/EGYY/198616d0a780388f8effd9b24a766529/raw/"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDY0NDVGRjNCMTk4NTA2MzMKUldRekJvVVpPLzlGWk5SVGpWVFJmMG4rWW91T01mQ0RGUTFiREdHWmhmNlcrYlRFeGQwWXZHWm4K"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.taurisample.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-sample",
        "width": 800,
        "height": 600
      }
    ]
  }
}